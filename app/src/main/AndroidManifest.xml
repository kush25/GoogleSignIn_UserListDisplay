<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hcl.googlesigninapp_miniapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_person_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_person_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoogleSignInApp_MiniApp"
        android:usesCleartextTraffic="true">

        <activity android:name=".userdetails.Mydtls"></activity>
        <activity android:name=".userdetails.UserDetailsActivity" />
        <activity android:name=".userdetails.UserActivity" android:launchMode="singleInstance" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>